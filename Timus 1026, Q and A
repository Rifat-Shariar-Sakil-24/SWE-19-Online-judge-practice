#include<bits/stdc++.h>
#define ll  long long
#define pb push_back
#define sp " "
#define maxi 1000000000
#define forn(i,n) for(long long i=0;i<n;i++)
using namespace std;


 
int main()
{  

   ll n; cin >> n;
   vector <ll> v(n);
   forn(i,n) cin >> v[i];
   sort(v.begin(),v.end());
   string s; cin >> s;
   ll m; cin >> m;
   while(m--)
   {
       ll i; cin >> i;
       cout << v[i-1] << endl;
   }
}
